def password_generator():
    import random
    length = int(input("How many characters would you like in your randomly generated password? "))
    characters = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890?!¤%&/()=?;:><_-:.;,ÄäÖöÅå^¨*"
    generated_password = "".join(random.sample(characters, length))
    print(generated_password)
